
{{#each this}}
<li class="test" data-id='{{id}}'>
   {{#if poster_path}}
   <img class="test" src="https://image.tmdb.org/t/p/w400{{poster_path}}" alt="{{original_title}}"
      data-type="{{media_type}}" data-src="{{id}}">
   {{else}}
   <img class="test" src="../images/noposter.jpg"
      data-src="{{id}}" height="400" />
   {{/if}}
   <h2 class="test">{{title}}</h1>
   <p class="test">{{release_date}}</p>
</li>
{{/each}}